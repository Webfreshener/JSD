{
	"elements":{
		"*": {
			"polymorphic": [{
				"type": "String",
				"restrict": "^[a-zA-Z-0-9_]+$"
				},
				{
				"type": "Object",
				"elements": {
					"id": {
						"type": "Number",
						"required": true
					},
					"name": {
						"type": "String",
						"required": true,
						"restrict": "^[a-zA-Z-0-9_]+$"
					},
					"description": {
						"type": "String",
						"restrict": "^[a-zA-Z-0-9_\\s]+$"
					}
					}
				},
				{
				"type": "Object",
				"elements": {
					"id": {
						"type": "Number",
						"required": true
					},
					"active": {
						"type": "Boolean",
						"required": true
					}
				}
			}]
		}
	}
}