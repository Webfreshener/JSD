{
  "NestedObjects": {
    "type": "Object",
    "required": false,
    "extensible": true,
    "properties": {
      "anArray": {
        "type": "Array",
        "required": true,
        "properties": [{
          "type": "String"
        }]
      },
      "anObject": {
        "type": "Object",
        "required": false,
        "extensible": false,
        "default": {
          "aString": "TEST",
          "aObject": {
            "someValue": 0
          },
          "aDeepObject": {
            "aDeepParam": "foo",
            "aDeeperObject": {
              "aDeepParam": "string2"
            }
          }
        },
        "properties": {
          "aString": {
            "type": "String",
            "required": true,
            "restrict": "^TEST+$"
          },
          "aObject": {
            "type": "Object",
            "required": true,
            "extensible": true
          },
          "aDeepObject": {
            "type": "Object",
            "required": true,
            "extensible": false,
            "properties": {
              "aDeepParam": {
                "type": "String",
                "required": true,
                "restrict": "^[a-zA-Z0-9\\\\s]+$"
              },
              "aDeeperObject": {
                "type": "Object",
                "required": false,
                "extensible": false,
                "properties": {
                  "aDeepParam": {
                    "type": "String",
                    "required": true,
                    "restrict": "^[a-zA-Z0-9\\\\s]+$"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}