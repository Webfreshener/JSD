{
	"properties": {
		"type": "Object",
		"required": true,
		"extensible": true,
		"properties": {
			"type": "Object",
			"required": false,
			"extensible": true,
			"properties": {
				"default": {
					"type": "Boolean",
					"required": true
				},
				"defaultFn": {
					"type": "String",
					"required": false,
					"restrict": "^(guid|uuid|uuidv4|now)+$"
				},
				"description": {
					"type": "String",
					"required": true,
					"restrict": "^((?!\"|\b|\\f|\\n|\\r|\\t|\\u).)*$"
				},
				"id": {
					"type": ["Boolean", "Object"],
					"required": false,
					"properties": [{
						"type": {
							"type": "String",
							"required": true,
							"restrict": "^(String|Number)+$"
						},
						"id": {
							"type": "Boolean",
							"required": true
						},
						"generated": {
							"type": "Boolean",
							"required": false
						}
					}]
				},
				"index": {
					"type": "Boolean",
					"required": false
				},
				"type": {
					"type": "String",
					"required": true,
					"restrict": "^(\\*|any|Array|Boolean|Buffer|Date|GeoPoint|Number|Object|String|[a-zA-Z-0-9_])+$"
				},
				"required": {
					"type": "Boolean",
					"required": true,
					"default": false
				},
				"length": {
					"type": ["Number", null],
					"required": true,
					"default": null
				},
				"precision": {
					"type": ["Number", null],
					"required": true,
					"default": null
				},
				"scale": {
					"type": ["Number", null],
					"required": true,
					"default": null
				}
			}
		}
	}
}