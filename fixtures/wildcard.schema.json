{
  "*": {
    "type": "Object",
    "required": true,
    "elements": {
      "*": {
        "type": "Object",
        "required": true,
        "extensible": true,
        "elements": {
          "*": {
            "type": ["Boolean", "String", "Number"]
          },
          "name": {
            "type": "String",
            "required": true
          }
        }
      }
    }
  }
}
			